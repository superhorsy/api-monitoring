{
    "template": "logstash-asrz-2.0",
    "index_patterns": [
        "logstash-asrz-2.0-*"
    ],
    "settings": {
        "number_of_shards": 1
    },
    "mappings": {
        "properties": {
            "@timestamp": {
                "type": "date"
            },
            "host": {
                "type": "keyword"
            },
            "path": {
                "type": "keyword"
            },
            "uri": {
                "type": "keyword"
            },
            "statusCode": {
                "type": "integer"
            },
            "contentType": {
                "type": "text"
            },
            "requestTime": {
                "type": "float"
            }
        }
    }
}
